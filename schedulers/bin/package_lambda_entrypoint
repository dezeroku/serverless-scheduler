#!/usr/bin/env bash
set -euo pipefail

RUNDIR="$(readlink -f "$(dirname "$0")")"
pushd "${RUNDIR}/.."

export COMPONENT_NAME=schedulers

# Make sure that common is in place
"${RUNDIR}/../../common/bin/package_lambda_entrypoint"

# Package
INTERMEDIATE_PACKAGING=true "${RUNDIR}/../../common/bin/package_lambda_runner.sh"
"${RUNDIR}/../../common/bin/package_lambda_concatenate.sh"
